{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "module":"CommonJS",
        "noImplicitAny": true,
        "outDir": "./build/",
        "rootDir": "src",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "ES5"
    },
    "include": [
        "src/**/*"
        ]
}